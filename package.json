{
  "dependencies": {
    "bootstrap": "4.5.0",
    "chart.js": "^4.4.2",
    "chartkick": "^5.0.1",
    "jquery": "3.5.1",
    "popper.js": "1.16.0",
    "rails_admin": "3.1.2",
    "sass": "^1.70.0",
    "startbootstrap-sb-admin-2": "^4.1.4"
  },
  "scripts": {
    
    "build:custom": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --public-path=assets",
    "build:custom2": "esbuild app/javascript/vendor/*.js --bundle --sourcemap --outdir=app/assets/builds --public-path=assets",
    "build:css": "sass ./app/assets/stylesheets/application.sass.scss:./app/assets/builds/application.css ./app/assets/stylesheets/rails_admin.scss:./app/assets/builds/rails_admin.css --no-source-map --load-path=node_modules"

  }
}
